{
  "name": "nydus-client",
  "version": "5.1.0",
  "description": "WebSocket client library for the nydus protocol, a simple RPC/PubSub protocol.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=22.12"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint --ext .js,.ts ./",
    "prepublishOnly": "pnpm run lint && pnpm run typecheck && pnpm run test && pnpm run build",
    "test": "mocha --import=tsx -R spec test/**.ts",
    "typecheck": "tsc --noEmit",
    "watch": "tsup --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tec27/nydus-client"
  },
  "keywords": [
    "websocket",
    "rpc",
    "pubsub",
    "websockets",
    "nydus"
  ],
  "author": "Travis Collins <travis@tec27.com> (http://tec27.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tec27/nydus-client/issues"
  },
  "homepage": "https://github.com/tec27/nydus-client",
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "backo2": "^1.0.2",
    "engine.io-client": "^6.6.4",
    "nanoid": "^5.1.6",
    "nydus-protocol": "^3.0.0",
    "ruta3": "^2.0.1",
    "tiny-typed-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@swc/core": "^1.15.7",
    "@types/backo2": "^1.0.4",
    "@types/chai": "^5.2.3",
    "@types/chai-as-promised": "^8.0.2",
    "@types/mocha": "^10.0.10",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "chai": "^6.2.2",
    "chai-as-promised": "^8.0.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "mocha": "^11.7.5",
    "nydus": "^5.3.0",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.26.2+sha512.0e308ff2005fc7410366f154f625f6631ab2b16b1d2e70238444dd6ae9d630a8482d92a451144debc492416896ed16f7b114a86ec68b8404b2443869e68ffda6"
}
